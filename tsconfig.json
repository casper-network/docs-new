{
    "extends": "@tsconfig/docusaurus/tsconfig.json",
    "compilerOptions": {
        "baseUrl": "./",
        "typeRoots": [
            "./types"
        ],
        "paths": {
            "@site/src/assets": [
                "./src/assets"
            ],
            "@site/src/components": [
                "./src/components"
            ],
            "@site/src/mocks": [
                "./src/mocks"
            ],
            "@site/src/pages": [
                "./src/pages"
            ],
            "@site/src/utils": [
                "./src/utils"
            ]
        }
    },
    "include": [
        "./src",
        "./types"
    ]
}
